{
  "name": "neetcoder",
  "version": "1.0.0",
  "description": "a discord bot",
  "module": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "release": "npm run migrate",
    "postbuild": "node dist/deploy-commands.js",
    "migrate": "drizzle-kit migrate",
    "deploy-commands": "tsx src/deploy-commands.ts",
    "format:check": "npx @biomejs/biome check",
    "format:write": "npx @biomejs/biome check --write",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaidenmagnan/neetcoder.git"
  },
  "author": "Jaiden Magnan",
  "license": " GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Jaidenmagnan/neetcoder/issues"
  },
  "homepage": "https://github.com/Jaidenmagnan/neetcoder#readme",
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "drizzle-kit": "^0.31.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "dotenv-cli": "^11.0.0"
  }
}
